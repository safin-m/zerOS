{
  "llvm-target": "i686-unknown-none",
  "data-layout": "e-m:e-p:32:32-i8:8:32-i16:16:32-i64:32-f64:32-i128:32-n8:16:32-S128",
  "arch": "x86",
  "os": "none",
  "executables": true,
  "linker-flavor": "ld",
  "linker": "i686-elf-ld",
  "pre-link-args": {
    "ld": ["-T", "./src/linker.ld"]
  },
  "post-link-args": {
    "ld": ["--oformat", "elf32-i386"]
  },
  "panic-strategy": "abort",
  "disable-redzone": true,
  "features": "+sse,+sse2",
  "target-pointer-width": "32"
}
